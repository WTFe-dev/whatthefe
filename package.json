  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-rss": "^2.0.3",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "html-minifier": "^4.0.0",
    "luxon": "^3.5.0",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.0.9",
    "terser": "^5.39.0"
  },
"scripts": {
    "clean": "rm -rf _site",
    "dev:tailwind": "tailwindcss -i src/assets/css/tailwind.css -o src/assets/css/styles.css --watch",
    "dev:eleventy": "eleventy --serve",
    "dev": "npm-run-all clean --parallel dev:*",
    "build:tailwind": "tailwindcss -i src/assets/css/tailwind.css -o src/assets/css/styles.css --minify",
    "build:eleventy": "eleventy",
    "build": "cross-env NODE_ENV=production npm-run-all clean build:tailwind build:eleventy",
    "serve": "eleventy --serve"
  },
  "keywords": [
    "eleventy",
    "11ty",
    "blog",
    "tailwindcss",
    "jamstack"
  ],
  "engines": {
    "node": ">=22.0.0"
  }  
